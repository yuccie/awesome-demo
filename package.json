{
  "name": "vue-spa-template-3.0",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "mock": "nodemon --watch mock build/mock.server.js",
    "build": "vue-cli-service build",
    "prod-server": "cross-env-shell NODE_ENV=production \"node build/server.js & npm run mock\"",
    "test-server": "cross-env-shell NODE_ENV=test \"node build/server.js & npm run mock\"",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit",
    "inspect": "vue inspect > output.js",
    "inspect:production": "vue inspect --mode production > output.production.js"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "cross-env": "^5.2.0",
    "express": "^4.16.3",
    "http-proxy-middleware": "^0.19.0",
    "serve-static": "^1.13.2",
    "vue": "^2.5.17",
    "vue-class-component": "^6.0.0",
    "vue-property-decorator": "^7.0.0",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@types/chai": "^4.1.0",
    "@types/mocha": "^5.2.4",
    "@vue/cli-plugin-babel": "^3.0.1",
    "@vue/cli-plugin-eslint": "^3.0.1",
    "@vue/cli-plugin-typescript": "^3.0.1",
    "@vue/cli-plugin-unit-mocha": "^3.0.1",
    "@vue/cli-service": "^3.0.1",
    "@vue/eslint-config-standard": "^3.0.1",
    "@vue/eslint-config-typescript": "^3.0.1",
    "@vue/test-utils": "^1.0.0-beta.20",
    "chai": "^4.1.2",
    "copy-webpack-plugin": "^4.5.2",
    "lint-staged": "^7.2.2",
    "mockjs": "^1.0.1-beta3",
    "node-sass": "^4.9.0",
    "nodemon": "^1.18.4",
    "postcss-preset-env": "^5.3.0",
    "require-dir": "^1.0.0",
    "sass-loader": "^7.0.1",
    "serve": "^10.0.0",
    "svg-sprite-loader": "^3.9.2",
    "typescript": "^3.0.0",
    "vue-template-compiler": "^2.5.17"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
